{
  "version": 2,
  "functions": {
    "index.js": {
      "runtime": "@vercel/node@18"
    },
    "api/edge.js": {
      "runtime": "@vercel/edge@1",
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
